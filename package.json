{
  "name": "survey-api",
  "version": "2.6.2",
  "description": "",
  "main": "dist/main/server.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json && copyfiles -u 1 public/**/* dist/static/",
    "build:watch": "npm run build -- --watch",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "up": "concurrently --kill-others-on-fail \"npm run build:watch\" \"docker-compose up\"",
    "down": "docker-compose down",
    "test": "jest --passWithNoTests --runInBand",
    "test:unit": "npm run test -- -c jest-unit.config.js",
    "test:integration": "npm run test -- -c jest-integration.config.js",
    "test:staged": "npm run test -- --findRelatedTests",
    "test:ci": "npm run test -- --coverage",
    "lint": "eslint --ignore-path .gitignore --ext .ts,.js --fix",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^16.11.12",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "concurrently": "^6.4.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.3",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.0",
    "jest": "^27.4.4",
    "lint-staged": "^12.1.2",
    "mockdate": "^3.0.5",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.1",
    "typescript": "^4.5.3"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run lint",
      "npm run test:staged"
    ]
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "^4.2.1",
    "swagger-ui-express": "^4.3.0",
    "validator": "^13.7.0"
  },
  "engines": {
    "node": "14.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
