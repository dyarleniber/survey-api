{
  "name": "survey-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests --noStackTrace --runInBand",
    "test:unit": "npm run test -- -c jest-unit.config.js",
    "test:integration": "npm run test -- -c jest-integration.config.js",
    "test:staged": "npm run test -- --findRelatedTests",
    "test:ci": "npm run test -- --coverage",
    "lint": "eslint --ignore-path .gitignore --ext .ts,.js --fix",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^27.0.3",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^16.11.9",
    "@types/validator": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.3",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run lint",
      "npm run test:staged"
    ]
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "mongodb": "^4.2.0",
    "validator": "^13.7.0"
  }
}
